"""https://github.com/nicknash/teckit - TECkit Unicode conversion library"""

module(
    name = "teckit",
    version = "2.5.12",
    bazel_compatibility = [">=7.2.1"],
)

bazel_dep(name = "platforms", version = "1.0.0")
bazel_dep(name = "rules_cc", version = "0.2.4")
bazel_dep(name = "rules_cc_autoconf", version = "0.5.3")
bazel_dep(name = "zlib", version = "1.3.1.bcr.8")
bazel_dep(name = "libexpat", version = "2.7.1.bcr.1")

single_version_override(
    module_name = "libexpat",
    patches = ["//patches:libexpat_windows.patch"],
    patch_strip = 1,
)
